                        <input type="text" class="form-control" id="fullName" name="fullName"  placeholder="Full name" required>
                      </div>
                      <div class="col-md-3 mb-3">
                        <label for="validationDefault03">Phone Number</label>
                        <div class="input-group">
                          <input type="text" class="form-control" id="phoneNumber" name="phoneNumber" placeholder="Phone Number" required>
                        </div>
                      </div>
                    </div>

                    <div class="form-row">
                      <div class="col-md-7 mb-3">
                        <label for="validationDefault04">Email</label>
                        <div class="input-group">
                          <input type="text" class="form-control" id="emailAccount" name="emailAccount" placeholder="Email" required>
                        </div>
                      </div>
                    </div>

                    <div class="form-row">


                      <div class="col-md-3 mb-3">
                        <label for="validationDefault04">Student/Staff Number</label>
                        <div class="input-group">
                          <input type="text" class="form-control" id="studentNumber" name="studentNumber" placeholder="Student/Staff Number" required>
                        </div>
                      </div>

                      <div class="col-md-2 mb-3">
                        <label for="validationDefault04">Type</label>
                        <select id="typeAccount" name="typeAccount" class="form-control form-control-sm">
                          <option value="student">Student</option>
                          <option value="staff">Staff</option>
                        </select>
                      </div>
                    </div>



                    <div class="form-row">
                      <div class="col-md-7 mb-3">
                        <label for="validationDefault04">Username</label>
                        <div class="input-group">
                          <input type="text" class="form-control" id="usernameAccount" name="usernameAccount" placeholder="Username" required>
                        </div>
                      </div>
                    </div>

                    <div class="form-row">
                      <div class="col-md-3 mb-3">
                        <label for="validationDefault04">Password</label>
                        <input type="password" class="form-control" id="password" name="password" placeholder="Password">
                      </div>
                      <div class="col-md-3 mb-3">
                        <label for="validationDefault04">Confirm Password</label>
                        <input type="password" class="form-control" id="password2" name="password2" placeholder="Confirm Password">
                      </div>
                    </div>

                    <div class="ce" style="text-align: left; margin-right: 0; ">
                      <button type="submit" class="styleButton" style="cursor: pointer; margin: 10px 0px;width: 200px; font-size: 18px; margin-left:0px;">Sign up</button>
                    </div>

                  </form>
                </div>
